{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-tabs": "off",
        "no-mixed-spaces-and-tabs": "warn",
        "indent": ["warn", "tab"]
    }
}
